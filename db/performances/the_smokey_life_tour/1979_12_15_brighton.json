            "tokens": [
              {
                "kind": "text",
                "text": "verse variation"
              }
            ]
          },
          {
            "kind": "lineBreak"
          },
          {
            "kind": "lineBreak"
          },
          {
            "kind": "text",
            "id": "2",
            "tokens": [
              {
                "kind": "text",
                "text": "You can hear the boats go by"
              }
            ]
          },
          {
            "kind": "lineBreak"
          },
          {
            "kind": "text",
            "id": "3",
            "tokens": [
              {
                "kind": "text",
                "text": "You can hear the night beside her."
              }
            ]
