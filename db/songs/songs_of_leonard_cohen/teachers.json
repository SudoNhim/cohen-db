      "annotations": [
        {
          "user": "sudonhim",
          "content": [
            {
              "text": "Societies that embrace the scalpel have not fared well.",
              "kind": "text"
            }
          ],
          "id": "1"
        }
      ],
      "newAnchor": "21"
    },
    {
