  "content": {
    "kind": "simple",
    "content": {
      "fragments": [
        {
          "kind": "text",
          "id": "1",
          "tokens": [
            {
              "kind": "text",
              "text": "It is late afternoon."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "2",
          "tokens": [
            {
              "kind": "text",
              "text": "I have put Beethoven on."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "3",
          "tokens": [
            {
              "kind": "text",
              "text": "It is foolish to impute pain"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "4",
          "tokens": [
            {
              "kind": "text",
              "text": "to the intense sky"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "5",
          "tokens": [
            {
              "kind": "text",
              "text": "but that is what I have done."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "6",
          "tokens": [
            {
              "kind": "text",
              "text": "And I will impute loneliness"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "7",
          "tokens": [
            {
              "kind": "text",
              "text": "to the appearing moon."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "8",
          "tokens": [
            {
              "kind": "text",
              "text": "It is early night."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "9",
          "tokens": [
            {
              "kind": "text",
              "text": "Down in the lighted city"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "10",
          "tokens": [
            {
              "kind": "text",
              "text": "the tedious hunts begin."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "11",
          "tokens": [
            {
              "kind": "text",
              "text": "I have been assured"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "12",
          "tokens": [
            {
              "kind": "text",
              "text": "there is no cause for shame."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "13",
          "tokens": [
            {
              "kind": "text",
              "text": "I am not ashamed."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "14",
          "tokens": [
            {
              "kind": "text",
              "text": "I turn the music louder."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "15",
          "tokens": [
            {
              "kind": "text",
              "text": "There's the moon"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "16",
          "tokens": [
            {
              "kind": "text",
              "text": "in my room's window."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "17",
          "tokens": [
            {
              "kind": "text",
              "text": "I balance it on my thumb"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "18",
          "tokens": [
            {
              "kind": "text",
              "text": "and try to flip it over."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "19",
          "tokens": [
            {
              "kind": "text",
              "text": "It does not turn,"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "20",
          "tokens": [
            {
              "kind": "text",
              "text": "but still, my thumb"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "21",
          "tokens": [
            {
              "kind": "text",
              "text": "is not broken."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "22",
          "tokens": [
            {
              "kind": "text",
              "text": "I open the window."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "23",
          "tokens": [
            {
              "kind": "text",
              "text": "I make the music softer."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "24",
          "tokens": [
            {
              "kind": "text",
              "text": "I walk on Murray Hill."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "25",
          "tokens": [
            {
              "kind": "text",
              "text": "The moon needs no legend."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "26",
          "tokens": [
            {
              "kind": "text",
              "text": "It proclaims its interest"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "27",
          "tokens": [
            {
              "kind": "text",
              "text": "in time, in the immediate night."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "28",
          "tokens": [
            {
              "kind": "text",
              "text": "I decide to leave it alone."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "29",
          "tokens": [
            {
              "kind": "text",
              "text": "In my room"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "30",
          "tokens": [
            {
              "kind": "text",
              "text": "the music is turning"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "31",
          "tokens": [
            {
              "kind": "text",
              "text": "because I expect a friend."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "lineBreak"
        }
      ]
