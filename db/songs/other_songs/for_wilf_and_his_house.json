  "content": {
    "kind": "simple",
    "content": {
      "fragments": [
        {
          "kind": "text",
          "id": "1",
          "tokens": [
            {
              "kind": "text",
              "text": "When young the Christians told me"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "2",
          "tokens": [
            {
              "kind": "text",
              "text": "how we pinned Jesus"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "3",
          "tokens": [
            {
              "kind": "text",
              "text": "like a lovely butterfly against the wood,"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "4",
          "tokens": [
            {
              "kind": "text",
              "text": "and I wept beside paintings of Calvary"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "5",
          "tokens": [
            {
              "kind": "text",
              "text": "at velvet wounds"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "6",
          "tokens": [
            {
              "kind": "text",
              "text": "and delicate twisted feet."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "7",
          "tokens": [
            {
              "kind": "text",
              "text": "But he could not hang softly long,"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "8",
          "tokens": [
            {
              "kind": "text",
              "text": "your fighters so proud with bugles,"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "9",
          "tokens": [
            {
              "kind": "text",
              "text": "bending flowers with their silver stain,"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "10",
          "tokens": [
            {
              "kind": "text",
              "text": "and when I faced the Ark for counting,"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "11",
          "tokens": [
            {
              "kind": "text",
              "text": "trembling under the burning oil,"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "12",
          "tokens": [
            {
              "kind": "text",
              "text": "the meadow of running flesh turned sour"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "13",
          "tokens": [
            {
              "kind": "text",
              "text": "and I kissed away my gentle teachers,"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "14",
          "tokens": [
            {
              "kind": "text",
              "text": "warned my younger brothers."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "15",
          "tokens": [
            {
              "kind": "text",
              "text": "Among the young and turning-great"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "16",
          "tokens": [
            {
              "kind": "text",
              "text": "of the large nations, innocent"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "17",
          "tokens": [
            {
              "kind": "text",
              "text": "of the spiked wish and the bright crusade,"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "18",
          "tokens": [
            {
              "kind": "text",
              "text": "there I could sing my heathen tears"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "19",
          "tokens": [
            {
              "kind": "text",
              "text": "between the summersaults and chestnut battles,"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "20",
          "tokens": [
            {
              "kind": "text",
              "text": "love the distant saint"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "21",
          "tokens": [
            {
              "kind": "text",
              "text": "who fed his arm to flies,"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "22",
          "tokens": [
            {
              "kind": "text",
              "text": "mourn the crushed ant"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "23",
          "tokens": [
            {
              "kind": "text",
              "text": "and despise the reason of the heel."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "24",
          "tokens": [
            {
              "kind": "text",
              "text": "Raging and weeping are left on the early road."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "25",
          "tokens": [
            {
              "kind": "text",
              "text": "Now each in his holy hill"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "26",
          "tokens": [
            {
              "kind": "text",
              "text": "the glittering and hurting days are almost done."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "27",
          "tokens": [
            {
              "kind": "text",
              "text": "Then let us compare mythologies."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "28",
          "tokens": [
            {
              "kind": "text",
              "text": "I have learned my elaborate lie"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "29",
          "tokens": [
            {
              "kind": "text",
              "text": "of soaring crosses and poisoned thorns"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "30",
          "tokens": [
            {
              "kind": "text",
              "text": "and how my fathers nailed him"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "31",
          "tokens": [
            {
              "kind": "text",
              "text": "like a bat against a barn"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "32",
          "tokens": [
            {
              "kind": "text",
              "text": "to greet the autumn and late hungry ravens"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "33",
          "tokens": [
            {
              "kind": "text",
              "text": "as a hollow yellow sign."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "lineBreak"
        }
      ]
    }
  }
}