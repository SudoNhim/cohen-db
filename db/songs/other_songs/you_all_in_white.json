  "content": {
    "kind": "simple",
    "content": {
      "fragments": [
        {
          "kind": "text",
          "id": "1",
          "tokens": [
            {
              "kind": "text",
              "text": "Whatever cities are brought down,"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "2",
          "tokens": [
            {
              "kind": "text",
              "text": "I will always bring you poems,"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "3",
          "tokens": [
            {
              "kind": "text",
              "text": "and the fruit of orchards"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "4",
          "tokens": [
            {
              "kind": "text",
              "text": "I pass by."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "5",
          "tokens": [
            {
              "kind": "text",
              "text": "Strangers in your bed,"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "6",
          "tokens": [
            {
              "kind": "text",
              "text": "excluded by our grief,"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "7",
          "tokens": [
            {
              "kind": "text",
              "text": "listening to sleep-whispering,"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "8",
          "tokens": [
            {
              "kind": "text",
              "text": "will hear their passion beautifully explained,"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "9",
          "tokens": [
            {
              "kind": "text",
              "text": "and weep because they cannot kiss"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "10",
          "tokens": [
            {
              "kind": "text",
              "text": "your distant face."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "11",
          "tokens": [
            {
              "kind": "text",
              "text": "Lovers of my beloved,"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "12",
          "tokens": [
            {
              "kind": "text",
              "text": "watch how my words put on her lips like clothes,"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "13",
          "tokens": [
            {
              "kind": "text",
              "text": "how they wear her body like a rare shawl."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "14",
          "tokens": [
            {
              "kind": "text",
              "text": "Fruit is pyramided on the window-sill,"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "15",
          "tokens": [
            {
              "kind": "text",
              "text": "songs flutter against the disappearing wall."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "16",
          "tokens": [
            {
              "kind": "text",
              "text": "The sky of the city"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "17",
          "tokens": [
            {
              "kind": "text",
              "text": "is washed in the fire"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "18",
          "tokens": [
            {
              "kind": "text",
              "text": "of Lebanese cedar and gold."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "19",
          "tokens": [
            {
              "kind": "text",
              "text": "In smoky filigree cages"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "20",
          "tokens": [
            {
              "kind": "text",
              "text": "the apes and peacocks fret."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "21",
          "tokens": [
            {
              "kind": "text",
              "text": "Now the cages do not hold,"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "22",
          "tokens": [
            {
              "kind": "text",
              "text": "in the burning street man and animal"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "23",
          "tokens": [
            {
              "kind": "text",
              "text": "perish in each other's arms,"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "24",
          "tokens": [
            {
              "kind": "text",
              "text": "peacocks drown around the melting throne."
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "25",
          "tokens": [
            {
              "kind": "text",
              "text": "Is it the king"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "26",
          "tokens": [
            {
              "kind": "text",
              "text": "who lies beside you listening?"
            }
          ]
        },
        {
          "kind": "lineBreak"
        },
        {
          "kind": "text",
          "id": "27",
          "tokens": [
            {
              "kind": "text",
              "text": "Is it Solomon or David"
